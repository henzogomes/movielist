{
  "name": "movielist",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "docker:build": "docker compose up -d movielist-api --build",
    "docker:up": "docker-compose up movielist-api",
    "docker:daemon": "docker-compose up -d movielist-api",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose --profile testing up --build movielist-test",
    "docker:logs": "docker-compose logs -f movielist-api"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "sqlite3": "^5.1.7",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.23",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.28.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
